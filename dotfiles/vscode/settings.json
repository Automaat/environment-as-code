{
    "workbench.colorTheme": "Nord Light",
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "git.alwaysSignOff": true,
    "git.enableSmartCommit": true,
    "git.autoStageAllChanges": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "go.toolsManagement.autoUpdate": true,
    "redhat.telemetry.enabled": true,
    "yaml.schemas": {
        "https://appengine.allegrogroup.com/en/latest/schemas/tycho.json": "*/tycho.yaml"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.tabSize": 2,
    "[ocaml]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "cmake.configureOnOpen": true,
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "workbench.editor.enablePreview": false,
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[python]": {
        "editor.formatOnType": true,
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "workbench.iconTheme": "vscode-icons",
    "vs-kubernetes": {
        "vs-kubernetes.crd-code-completion": "enabled",
        "vs-kubernetes.minikube-show-information-expiration": "2025-07-08T14:50:38.195Z",
        "vscode-kubernetes.helm-path-mac": "/Users/marcin.skalski@konghq.com/.vs-kubernetes/tools/helm/darwin-arm64/helm",
        "vscode-kubernetes.minikube-path-mac": "/Users/marcin.skalski@konghq.com/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
    },
    "workbench.editorAssociations": {
        "*.md": "default"
    },
    "explorer.confirmDelete": false,
    "editor.guides.indentation": false,
    "docker.extension.enableComposeLanguageServer": false,
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        },
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "github.copilot.nextEditSuggestions.enabled": true,
    "makefile.configureOnOpen": true,
    "mise.binPath": "/etc/profiles/per-user/marcin.skalski@konghq.com/bin/mise",
    "git.confirmSync": false,
    "gitlens.ai.model": "vscode",
    "gitlens.ai.vscode.model": "copilot:gpt-4.1",
    "git.ignoreRebaseWarning": true,
    "window.confirmBeforeClose": "keyboardOnly",
    "claudeCode.preferredLocation": "panel",
    "claudeCode.selectedModel": "default",
    "editor.fontFamily": "IosevkaTerm Nerd Font Mono",
    "terminal.integrated.fontFamily": "IosevkaTerm Nerd Font Mono",
    "python.languageServer": "Default",
    "svelte.enable-ts-plugin": true,
    "workbench.preferredHighContrastColorTheme": "Everforest Light",
    "workbench.preferredHighContrastLightColorTheme": "Everforest Light",
    "workbench.preferredLightColorTheme": "Everforest Light",
    "everforest.darkContrast": "soft",
    "everforest.lightContrast": "soft",
    "editor.formatOnSave": true,
    "everforest.highContrast": true
}
